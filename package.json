{
  "name": "bw-lab",
  "version": "0.1.0",
  "description": "3D Map & Replay View for Starcraft Broodwar",
  "main": "electron/index.js",
  "scripts": {
    "test": "jest --watchAll",
    "test:once": "jest --runInBand",
    "test:debug": "node --inspect node_modules/jest/bin/jest --runInBand",
    "clear-cache": "rm -rf .cache dist",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist -- --dir -c.compression=store -c.mac.identity=null",
    "dev": "electron-webpack dev"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imbateam-gg/bw-lab.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/imbateam-gg/bw-lab/issues"
  },
  "homepage": "https://github.com/imbateam-gg/bw-lab#readme",
  "electronWebpack": {
    "commonSourceDirectory": "src/common",
    "staticSourceDirectory": "bwdata",
    "title": "Titan Reactor",
    "main": {
      "sourceDirectory": "src/main"
    },
    "renderer": {
      "sourceDirectory": "src/renderer",
      "webpackConfig": "src/build/webpack.renderer.additions.js"
    }
  },
  "dependencies": {
    "@eliasku/binpack": "^1.0.1",
    "bl": "^4.0.3",
    "buffer": "^5.6.0",
    "concat": "1.0.3",
    "d3-ease": "1.0.6",
    "dat.gui": "0.7.7",
    "electron-is-dev": "^1.2.0",
    "iconv-lite": "0.6.1",
    "iscript": "^1.0.2",
    "jssuh": "1.4.0",
    "ks": "^2.0.4",
    "npm": "^6.14.8",
    "pngjs": "^5.0.0",
    "ramda": "0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.2.0",
    "sanitize-filename": "^1.6.3",
    "scm-extractor": "^1.0.6",
    "three": "^0.120.1",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-jest": "^26.3.0",
    "dotenv": "^8.2.0",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.1",
    "electron-webpack": "^2.8.2",
    "jest": "^26.4.2",
    "parcel-plugin-static-files-copy": "^2.5.0",
    "source-map-support": "^0.5.19",
    "webpack": "^4.44.2",
    "worker-loader": "^2.0.0"
  }
}
