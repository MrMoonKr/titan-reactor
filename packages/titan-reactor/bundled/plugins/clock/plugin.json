{
    "name": "Classic Clock",
    "version": "0.1",
    "author": "DarkMatter",
    "channels": {
        "@replay/ready": [{
            "type": "web-component",
            "url": "{style.value}.html",
            "pointerInteraction": false,
            "layout.bottom": "minimapHeight",
            "layout.width": "minimapWidth",
            "layout.height": "content",
            "access.read": ["replay.position"]
        }]
    },
    "userConfig": {
        "style": {
            "value": "classic",
            "options": ["classic", "modern"]
        },
        "fontSize": {
            "value": "16px"
        }
    }
}