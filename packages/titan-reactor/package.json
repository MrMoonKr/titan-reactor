{
  "name": "titan-reactor",
  "productName": "Titan Reactor",
  "version": "0.1.3",
  "description": "3D Map & Replay View for Starcraft Broodwar",
  "main": "src/main/index.js",
  "scripts": {
    "test": "jest --watchAll",
    "test:once": "jest --runInBand",
    "test:debug": "node --inspect node_modules/jest/bin/jest --runInBand",
    "clear-cache": "rm -rf .cache dist",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist -- --dir -c.compression=store -c.mac.identity=null",
    "dev": "electron-webpack dev",
    "dist:run": "yarn compile && electron dist/main/main.js",
    "lint": "yarn eslint ."
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imbateam-gg/bw-lab.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/imbateam-gg/bw-lab/issues"
  },
  "homepage": "https://github.com/imbateam-gg/bw-lab#readme",
  "electronWebpack": {
    "commonSourceDirectory": "src/common",
    "title": "Titan Reactor",
    "main": {
      "sourceDirectory": "src/main"
    },
    "renderer": {
      "sourceDirectory": "src/renderer",
      "webpackConfig": "src/build/renderer.config.js"
    }
  },
  "build": {
    "appId": "imbateam.titan-reactor",
    "productName": "Titan Reactor",
    "mac": {
      "category": "starcraft replay viewer"
    },
    "extraResources": [
      {
        "from": "static",
        "to": "static"
      }
    ]
  },
  "dependencies": {
    "bl": "^5.0.0",
    "buffer": "^6.0.3",
    "bw-chk-modified": "1.2.1",
    "camera-controls": "^1.25.3",
    "casclib": "^2.0.1",
    "concat": "1.0.3",
    "d3-ease": "1.0.6",
    "downgrade-replay": "^1.0.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^1.2.0",
    "iconv-lite": "0.6.3",
    "iscript": "^1.0.2",
    "lodash.debounce": "^4.0.8",
    "lodash.shuffle": "^4.2.0",
    "postprocessing": "^6.23.1",
    "preact": "^10.5.15",
    "react-error-boundary": "^3.1.3",
    "scm-extractor": "^1.0.6",
    "three": "0.133.1",
    "webpack": "4.42.1",
    "winreg": "^1.2.4",
    "zustand": "3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/bl": "^5.0.2",
    "@types/concat-stream": "^1.6.1",
    "@types/d3-ease": "^3.0.0",
    "@types/lodash.shuffle": "^4.2.6",
    "@types/three": "^0.133.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.2.2",
    "dotenv": "^8.2.0",
    "electron": "15.1.1",
    "electron-builder": "^22.8.1",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.2.4",
    "prettier": "^2.4.1",
    "source-map-support": "^0.5.19",
    "typescript": "^4.4.4",
    "worker-loader": "^2.0.0"
  }
}
