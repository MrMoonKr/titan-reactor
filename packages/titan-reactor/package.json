{
  "name": "titan-reactor",
  "productName": "Titan Reactor",
  "version": "0.1.3",
  "description": "3D Map & Replay View for Starcraft Broodwar",
  "main": "src/main/index.js",
  "scripts": {
    "test": "jest --watchAll",
    "test:once": "jest --runInBand",
    "test:debug": "node --inspect node_modules/jest/bin/jest --runInBand",
    "clear-cache": "rm -rf .cache dist",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist -- --dir -c.compression=store -c.mac.identity=null",
    "dev": "electron-webpack dev"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imbateam-gg/bw-lab.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/imbateam-gg/bw-lab/issues"
  },
  "homepage": "https://github.com/imbateam-gg/bw-lab#readme",
  "electronWebpack": {
    "commonSourceDirectory": "src/common",
    "title": "Titan Reactor",
    "main": {
      "sourceDirectory": "src/main"
    },
    "renderer": {
      "sourceDirectory": "src/renderer",
      "webpackConfig": "src/build/webpack.renderer.additions.js"
    }
  },
  "build": {
    "appId": "imbateam.titan-reactor",
    "productName": "Titan Reactor",
    "mac": {
      "category": "starcraft replay viewer"
    },
    "extraResources": [
      {
        "from": "static",
        "to": "static"
      }
    ]
  },
  "dependencies": {
    "bl": "^4.0.3",
    "buffer": "^5.6.0",
    "bw-chk-modified": "1.2.1",
    "camera-controls": "^1.25.3",
    "casclib": "^2.0.1",
    "concat": "1.0.3",
    "d3-ease": "1.0.6",
    "dat.gui": "0.7.7",
    "downgrade-replay": "^1.0.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^1.2.0",
    "hold-event": "^0.0.1",
    "iconv-lite": "0.6.1",
    "iscript": "^1.0.2",
    "jssuh": "1.4.0",
    "lodash.debounce": "^4.0.8",
    "lodash.shuffle": "^4.2.0",
    "pngjs": "^5.0.0",
    "postprocessing": "^6.19.3",
    "ramda": "0.27.0",
    "react": "17.0.1",
    "react-autosuggest": "^10.0.4",
    "react-color": "^2.19.3",
    "react-dom": "17.0.1",
    "react-dropzone": "^11.2.0",
    "react-tooltip": "^4.2.17",
    "sanitize-filename": "^1.6.3",
    "scm-extractor": "^1.0.6",
    "three": "0.125.2",
    "webpack": "4.42.1",
    "winreg": "^1.2.4",
    "zustand": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-jest": "^26.3.0",
    "dotenv": "^8.2.0",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.1",
    "electron-webpack": "^2.8.2",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.4.2",
    "parcel-plugin-static-files-copy": "^2.5.0",
    "source-map-support": "^0.5.19",
    "worker-loader": "^2.0.0"
  }
}
