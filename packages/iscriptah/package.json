{
  "name": "iscriptah",
  "version": "1.0.4",
  "license": "MIT",
  "author": "Alex Pineda <alexpineda@fastmail.com>",
  "build": {
    "appId": "imbateam.iscriptah",
    "productName": "iscriptAH!",
    "mac": {
      "category": "iscript animation"
    },
    "extraResources": [
      {
        "from": "data",
        "to": "data"
      }
    ]
  },
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "electronWebpack": {
    "whiteListedModules": [
      "react-redux"
    ],
    "renderer": {
      "webpackConfig": "./build/webpack.renderer.js"
    }
  },
  "dependencies": {
    "@babel/preset-env": "7.12.1",
    "@reduxjs/toolkit": "^1.5.0",
    "babel-loader": "8.1.0",
    "balloon-css": "^1.2.0",
    "bl": "^4.0.3",
    "bw-chk-modified": "1.2.1",
    "dotenv": "8.2.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-is-dev": "1.2.0",
    "titan-reactor-shared": "^1.0.0",
    "iscript": "^1.0.2",
    "pattern.css": "^1.0.0",
    "ramda": "^0.27.1",
    "react": "^16.14.0",
    "react-combine-reducers": "^1.0.4",
    "react-dom": "^16.14.0",
    "react-dropzone": "11.2.4",
    "react-redux": "7.2.2",
    "redux-actions": "^2.6.5",
    "source-map-support": "^0.5.16",
    "tailwindcss": "^1.9.2",
    "three": "^0.121.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@svgr/webpack": "^5.4.0",
    "@types/react": "^16.9.53",
    "electron": "8.2.0",
    "electron-builder": "^22.9.1",
    "electron-webpack": "^2.8.2",
    "eslint": "^7.16.0",
    "typescript": "^4.0.3",
    "webpack": "~4.42.1",
    "webpack-glsl-loader": "^1.0.1"
  }
}